<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 자주 사용하는 type 의 별칭 지정하기 -->
	<typeAliases>
		<typeAlias type="com.spoiler.movie.Dto.MovieDto" alias="movieDto"/>
		<typeAlias type="com.spoiler.movie.Dto.MovieCommentDto" alias="movieCommentDto"/>
		<typeAlias type="com.spoiler.movie.Dto.RecommendDto" alias="recommendDto"/>
		<typeAlias type="com.spoiler.movie.users.Dto.UsersDto" alias="usersDto"/>
		<typeAlias type="com.spoiler.movie.Dto.PopupDto" alias="popupDto"/>
		<typeAlias type="com.spoiler.movie.favorite.Dto.FavoriteDto" alias="favoriteDto"/>

	</typeAliases>
	
	<!-- mapper 설정 -->
	<mappers>
		<mapper resource="com/spoiler/movie/mybatis/MovieMapper.xml"/>
		<mapper resource="com/spoiler/movie/mybatis/MovieCommentMapper.xml"/>
		<mapper resource="com/spoiler/movie/mybatis/RecommendMapper.xml"/>
		<mapper resource="com/spoiler/movie/mybatis/UsersMapper.xml"/>
		<mapper resource="com/spoiler/movie/mybatis/AdminMapper.xml"/>
		<mapper resource="com/spoiler/movie/mybatis/FavoriteMapper.xml"/>
	</mappers>
</configuration>




